{
  "AWSEBDockerrunVersion": 1,
  "volumes": [
    {
      "name": "fc-deploy-eb",
      "host": {
        "sourcePath": "var/app/current/docker_eb"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "fc-deploy-eb",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 4040
        }
      ]
    }
  ]
}
